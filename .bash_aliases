alias sleep='systemctl suspend'
alias goodnight='systemctl suspend'
alias suspend='systemctl suspend'
alias update='sudo apt update'
alias upgrade='sudo apt upgrade'
alias recycle='rm -rf ~/.local/share/Trash'
alias install='sudo apt-get install'
alias ols="ls -la --color | awk '{k=0;for(i=0;i<=8;i++)k+=((substr(\$1,i+2,1)~/[rwx]/)*2^(8-i));if(k)printf(\" %0o \",k);print}'"
alias upup=' sudo apt update && sudo apt upgrade'
alias ..='cd ..'
alias ~='cd ~'
alias root='sudo su'
alias df='df -h'
alias aliases='nano .bash_aliases'
alias sa='source .bash_aliases'
alias sudo='sudo'
alias folder='mkdir'
alias dejavu='echo'
alias time='notify-send "Time:" $(date +%H:%M)'
alias sweep='clear'
alias sudu='echo try again'
alias sodu='echo nope'
